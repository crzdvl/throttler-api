{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-restricted-syntax": 0,
        "max-len": ["error", { "code": 120 }],
        "guard-for-in": 0,
        "no-await-in-loop": 0,
        "one-var": 0
    }
}
